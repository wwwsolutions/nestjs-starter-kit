{
  "name": "wwwsolutions-nest-starter",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "pre-commit": "lint-staged",
    "pre-push": "lint-staged",
    "nx": "nx",
    "build": "nx build",
    "test": "nx test",
    "lint": "nx workspace-lint && nx lint",
    "lint-all": "nx run-many --all --target=lint",
    "affected:lint-all": "nx affected:lint --base=main~1 --head=main --parallel=6 --verbose",
    "format": "nx format:write",
    "format:check": "nx format:check",
    "dep-graph": "nx dep-graph",
    "affected": "nx affected --base=main",
    "affected:dep-graph": "nx affected:dep-graph --base=main",
    "affected:apps": "nx affected:apps --base=main",
    "affected:libs": "nx affected:libs --base=main",
    "affected:build": "nx affected:build --base=main",
    "affected:test": "nx affected:test --base=main",
    "affected:lint": "nx affected:lint --base=main --parallel=6",
    "clean:logs": "rm -rf logs",
    "clean:dist": "rm -rf dist",
    "clean:temp": "rm -rf temp",
    "clean:node_modules": "rm -rf node_modules",
    "clean": "yarn clean:node_modules && yarn clean:dist && yarn clean:temp && yarn clean:logs"
  },
  "dependencies": {
    "@nestjs/common": "^8.2.0",
    "@nestjs/config": "^1.1.0",
    "@nestjs/core": "^8.2.0",
    "@nestjs/platform-express": "^8.2.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.2",
    "dotenv-cli": "^4.1.1",
    "joi": "^17.5.0",
    "nest-winston": "^1.6.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.0.0",
    "tslib": "^2.0.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@angular/cli": "^13.1.2",
    "@nestjs/schematics": "^8.0.4",
    "@nestjs/testing": "^8.2.0",
    "@nrwl/cli": "13.4.1",
    "@nrwl/eslint-plugin-nx": "13.4.1",
    "@nrwl/jest": "13.4.1",
    "@nrwl/js": "13.4.1",
    "@nrwl/linter": "13.4.1",
    "@nrwl/nest": "^13.4.1",
    "@nrwl/node": "13.4.1",
    "@nrwl/tao": "13.4.1",
    "@nrwl/workspace": "13.4.1",
    "@types/jest": "27.0.2",
    "@types/node": "14.14.33",
    "@typescript-eslint/eslint-plugin": "~5.3.0",
    "@typescript-eslint/parser": "~5.3.0",
    "eslint": "8.2.0",
    "eslint-config-prettier": "8.1.0",
    "husky": "^7.0.0",
    "jest": "27.2.3",
    "lint-staged": "^12.1.4",
    "prettier": "^2.3.1",
    "ts-jest": "27.0.5",
    "ts-node": "~10.3.0",
    "typescript": "~4.4.3"
  },
  "lint-staged": {
    "*.{js,json,md,ts}": [
      "npm run affected:lint --uncommitted",
      "npm run format --uncommitted"
    ]
  },
  "packageManager": "yarn@3.1.1"
}
