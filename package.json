{
  "name": "wwwsolutions-nest-starter",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "pre-commit": "lint-staged",
    "pre-push": "lint-staged",
    "nx": "nx",
    "build": "nx build",
    "test": "nx test",
    "lint": "nx workspace-lint && nx lint",
    "lint-all": "nx run-many --all --target=lint",
    "affected:lint-all": "nx affected:lint --base=main~1 --head=main --parallel=6 --verbose",
    "format": "nx format:write",
    "format:check": "nx format:check",
    "dep-graph": "nx dep-graph",
    "affected": "nx affected --base=main",
    "affected:dep-graph": "nx affected:dep-graph --base=main",
    "affected:apps": "nx affected:apps --base=main",
    "affected:libs": "nx affected:libs --base=main",
    "affected:build": "nx affected:build --base=main",
    "affected:test": "nx affected:test --base=main",
    "affected:lint": "nx affected:lint --base=main --parallel=6",
    "clean:logs": "rm -rf logs",
    "clean:dist": "rm -rf dist",
    "clean:temp": "rm -rf temp",
    "clean:node_modules": "rm -rf node_modules",
    "clean": "npm run clean:node_modules && npm run clean:dist && npm run clean:temp && npm run clean:logs",
    "dev": "nx run api:serve",
    "api:build:production": "npx nx build api --prod",
    "api:serve:production": "cross-env NODE_ENV=production WINSTON_LEVEL_CONSOLE=verbose WINSTON_PRETTY_PRINT=true WINSTON_LEVEL_FILE=silly API_INTEGRATION_TYPE=ApiIntegrationGraphqlPrisma node ./dist/apps/api/main.js",
    "docker:build": "docker build . -t courses/api",
    "docker:run": "docker run -it -p 8000:3000 courses/api",
    "api:deploy": "nx deploy api"
  },
  "dependencies": {
    "@nestjs/common": "^8.2.0",
    "@nestjs/config": "^1.1.0",
    "@nestjs/core": "^8.2.0",
    "@nestjs/graphql": "^9.1.2",
    "@nestjs/jwt": "8.0.0",
    "@nestjs/platform-express": "^8.2.0",
    "apollo-server-express": "^3.6.1",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.2",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^4.1.1",
    "graphql": "^15.8.0",
    "graphql-type-json": "^0.3.2",
    "joi": "^17.5.0",
    "joi-password": "^2.0.3",
    "nest-winston": "^1.6.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.0.0",
    "tslib": "^2.0.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@angular/cli": "^13.1.2",
    "@nestjs/schematics": "^8.0.4",
    "@nestjs/testing": "^8.2.0",
    "@nrwl/cli": "13.4.1",
    "@nrwl/eslint-plugin-nx": "13.4.1",
    "@nrwl/jest": "13.4.1",
    "@nrwl/js": "13.4.1",
    "@nrwl/linter": "13.4.1",
    "@nrwl/nest": "^13.4.1",
    "@nrwl/node": "13.4.1",
    "@nrwl/tao": "13.4.1",
    "@nrwl/workspace": "13.4.1",
    "@prisma/client": "^3.8.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/jest": "27.0.2",
    "@types/node": "14.14.33",
    "@typescript-eslint/eslint-plugin": "~5.3.0",
    "@typescript-eslint/parser": "~5.3.0",
    "eslint": "8.2.0",
    "eslint-config-prettier": "8.1.0",
    "husky": "^7.0.0",
    "jest": "27.2.3",
    "lint-staged": "^12.1.4",
    "prettier": "^2.3.1",
    "prisma": "^3.8.0",
    "prisma-nestjs-graphql": "^14.6.1",
    "ts-jest": "27.0.5",
    "ts-node": "~10.3.0",
    "typescript": "~4.4.3"
  },
  "lint-staged": {
    "*.{js,json,md,ts}": [
      "npm run affected:lint --uncommitted",
      "npm run format --uncommitted"
    ]
  }
}
