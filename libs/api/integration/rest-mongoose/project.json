{
  "root": "libs/api/integration/rest-mongoose",
  "sourceRoot": "libs/api/integration/rest-mongoose/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/node:package",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/api/integration/rest-mongoose",
        "tsConfig": "libs/api/integration/rest-mongoose/tsconfig.lib.json",
        "packageJson": "libs/api/integration/rest-mongoose/package.json",
        "main": "libs/api/integration/rest-mongoose/src/index.ts",
        "assets": ["libs/api/integration/rest-mongoose/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/api/integration/rest-mongoose/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/api/integration/rest-mongoose"],
      "options": {
        "jestConfig": "libs/api/integration/rest-mongoose/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": ["type:integration", "scope:api"]
}
