{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2017", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "downlevelIteration": true,
    "noErrorTruncation": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "preserveConstEnums": true,
    "removeComments": true,
    "strict": true,
    "strictFunctionTypes": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": true,
    "paths": {
      "@wwwsolutions/api/config/app": ["libs/api/config/app/src/index.ts"],
      "@wwwsolutions/api/config/codegen": [
        "libs/api/config/codegen/src/index.ts"
      ],
      "@wwwsolutions/api/config/docker": [
        "libs/api/config/docker/src/index.ts"
      ],
      "@wwwsolutions/api/config/features": [
        "libs/api/config/features/src/index.ts"
      ],
      "@wwwsolutions/api/config/integrations": [
        "libs/api/config/integrations/src/index.ts"
      ],
      "@wwwsolutions/api/config/swagger": [
        "libs/api/config/swagger/src/index.ts"
      ],
      "@wwwsolutions/api/core": ["libs/api/core/src/index.ts"],
      "@wwwsolutions/api/data-access/core": [
        "libs/api/data-access/core/src/index.ts"
      ],
      "@wwwsolutions/api/data-access/models": [
        "libs/api/data-access/models/src/index.ts"
      ],
      "@wwwsolutions/api/data-access/prisma": [
        "libs/api/data-access/prisma/src/index.ts"
      ],
      "@wwwsolutions/api/domain/article/data-access": [
        "libs/api/domain/article/data-access/src/index.ts"
      ],
      "@wwwsolutions/api/domain/article/dtos": [
        "libs/api/domain/article/dtos/src/index.ts"
      ],
      "@wwwsolutions/api/domain/article/feature": [
        "libs/api/domain/article/feature/src/index.ts"
      ],
      "@wwwsolutions/api/domain/authentication/data-access": [
        "libs/api/domain/authentication/data-access/src/index.ts"
      ],
      "@wwwsolutions/api/domain/authentication/feature": [
        "libs/api/domain/authentication/feature/src/index.ts"
      ],
      "@wwwsolutions/api/domain/users/data-access": [
        "libs/api/domain/users/data-access/src/index.ts"
      ],
      "@wwwsolutions/api/domain/users/feature": [
        "libs/api/domain/users/feature/src/index.ts"
      ],
      "@wwwsolutions/api/integration/graphql-prisma": [
        "libs/api/integration/graphql-prisma/src/index.ts"
      ],
      "@wwwsolutions/api/integration/rest-mongoose": [
        "libs/api/integration/rest-mongoose/src/index.ts"
      ],
      "@wwwsolutions/shared/data-access-api": [
        "libs/shared/data-access-api/src/index.ts"
      ],
      "@wwwsolutions/shared/types": ["libs/shared/types/src/index.ts"],
      "@wwwsolutions/shared/utils": ["libs/shared/utils/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
